let Functor = ./../Functor/Type

let TreeBase = ./Type

let List/map = ../List/map

in    λ(d : Type)
    →   { map =
              λ(a : Type)
            → λ(b : Type)
            → λ(fn : a → b)
            → λ(tb : TreeBase d a)
            → tb ⫽ { subtrees = List/map a b fn tb.subtrees }
        }
      : Functor (TreeBase d)
