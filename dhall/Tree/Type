{-
../TreeBase/Type = λ(d : Type) → λ(a : Type) → { data : d, subtrees : List a }
../TreeBase/Type t = λ(a : Type) → { data : t, subtrees : List a }
../Mu/Type = λ(f : Type → Type) → ∀(a' : Type) → (f a' → a') → a'
../Mu/Type (../TreeBase/Type t) = ∀(a' : Type) → ({ data : t, subtrees : List a' } -> a') -> a'
-}

λ(t : Type) → ../Mu/Type (../TreeBase/Type t)
