
  λ(a : Type)
→ λ(i : a)
→ λ(l : List a)
→ let List/steppable = ./steppable
  
  in  merge
      { Both =
            λ(both : { head : a, tail : List a })
          → let f = λ(x : a) → λ(xs : List a) → [ x, i ] # xs
            
            in  List/fold a both.tail (List a) f [ both.head ]
      , Neither =
          λ(_ : {}) → [] : List a
      }
      ((List/steppable a).project l)
